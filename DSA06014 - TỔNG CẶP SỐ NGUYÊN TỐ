#include <bits/stdc++.h>
#define f0(i, a) for(int i = 0; i < a; i++)
#define f1(i, a) for(int i = 1; i <= a; i++)
#define read freopen("input.txt", "r", stdin); freopen ("output.txt", "w", stdout);
using namespace std;

int MAX = 1e6 + 7;
vector <int> a(MAX, 1);
void ktao(){
    a[0] = a[1] = 0;
    for(int i = 2; i < sqrt(MAX); i++){
        for(int j = i * i; j < MAX; j += i){
            a[j] = 0;
        }
    } 
}

void solve(){
    int n; cin >> n;
    int check = 0;
    for(int i = 2; i <= n / 2; i++){
        if(a[i] && a[n - i]){
            check = 1;
            cout << i << " " << n - i << endl;
            break;
        }
    }
    if(check == 0) cout << "-1\n";
}

int main(){
    //read
    ktao();
    int t = 1;
    cin >> t;
    while(t--){
        solve();
    }
}
